<table class="table">
  <thead>
    <tr>
      <th>Nome</th>
      <th>Cidade de cobran√ßa</th>      
      <th>Telefone</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @conta.each do |contum| %>
      <tr>
        <td><%= contum.to_s %></td>
        <td><%= contum.cidadecobr_cidade %></td>
        <td><%= contum.telefone %></td>
        <td>
          <%= link_to 'Ver', contum, :class => 'btn btn-default btn-sm' %>
          <%= link_to 'Editar', edit_contum_path(contum), :class => 'btn btn-default btn-sm' %>
          <%= link_to 'Excluir', contum, method: :delete, data: { confirm: 'Tem certeza?' }, :class => 'btn btn-default btn-sm' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>