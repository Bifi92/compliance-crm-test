          <td><%= contato.cep_corre %></td>
          <td><%= contato.bairro_corpo %></td>
          <td><%= contato.logradouro %></td>
          <td><%= contato.rua_corpo %></td>
          <td><%= contato.numero_corpo %></td>
          <td><%= contato.complemento_corpo %></td>
          <td><%= contato.cep_corpo %></td>
          <td><%= contato.esporte_preferido %></td>
          <td><%= contato.hobby %></td>
          <td><%= contato.excluido %></td>
          <td><%= contato.origem_lead %></td>

          <td><%= link_to 'Show', contato, :class => 'btn btn-default btn-sm' %></td>
          <td><%= link_to 'Edit', edit_contato_path(contato), :class => 'btn btn-default btn-sm' %></td>
          <td><%= link_to 'Destroy', contato, method: :delete, data: { confirm: 'Are you sure?' }, :class => 'btn btn-default btn-sm' %></td>
        </tr>
    </tbody>
  </table>
<% end %>