        <th>Bairro corre</th>
        <th>Rua corre</th>
        <th>Numero corre</th>
        <th>Complemento corre</th>
        <th>Cep corre</th>
        <th>Bairro corpo</th>
        <th>Logradouro</th>
        <th>Rua corpo</th>
        <th>Numero corpo</th>
        <th>Complemento corpo</th>
        <th>Cep corpo</th>
        <th>Esporte preferido</th>
        <th>Hobby</th>
        <th>Excluido</th>
        <th>Origem lead</th>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      
        <tr>
          <td><%= contato.contum %></td>
          <td><%= contato.contato %></td>
          <td><%= contato.saudacao %></td>
          <td><%= contato.sexo %></td>
          <td><%= contato.titulo %></td>
          <td><%= contato.nome %></td>
          <td><%= contato.sobrenome %></td>
          <td><%= contato.data_nascimento %></td>
          <td><%= contato.empresa %></td>
          <td><%= contato.departamento %></td>
          <td><%= contato.cpf %></td>
          <td><%= contato.descricao %></td>
          <td><%= contato.email_pessoal %></td>
          <td><%= contato.email_corporativo %></td>
          <td><%= contato.nome_assistente %></td>
          <td><%= contato.telefone_assistente %></td>
          <td><%= contato.data_devolucao_email %></td>
          <td><%= contato.motivo_devolucao_email %></td>
          <td><%= contato.recusar_emails %></td>
          <td><%= contato.fax %></td>
          <td><%= contato.recusar_fax %></td>
          <td><%= contato.telefone_residencial %></td>
          <td><%= contato.telefone_comercial %></td>
          <td><%= contato.celular %></td>
          <td><%= contato.outro_telefone %></td>
          <td><%= contato.recusar_telefonemas %></td>
          <td><%= contato.bairro_corre %></td>
          <td><%= contato.rua_corre %></td>
          <td><%= contato.numero_corre %></td>
