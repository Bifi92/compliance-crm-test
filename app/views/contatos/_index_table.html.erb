<table class="table">
  <thead>
    <tr>
      <th>Nome</th>
      <th>Contum</th>
      <th>Telefone comercial</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @contatos.each do |contato| %>
      <tr>
        <td><%= contato.to_s %></td>
        <td><%= contato.contum %></td>
        <td><%= contato.telefone_comercial %></td>

        <td><%= link_to 'Ver', contato, :class => 'btn btn-default btn-sm' %></td>
        <td><%= link_to 'Editar', edit_contato_path(contato), :class => 'btn btn-default btn-sm' %></td>
        <td><%= link_to 'Excluir', contato, method: :delete, data: { confirm: 'Tem certeza?' }, :class => 'btn btn-default btn-sm' %></td>
      </tr>
    <% end %>  
    </tbody>
  </table>