<div class="panel panel-info">
  <div class="panel-heading">
     <h6 class="panel-title">Estado</h6>
  </div>
  <div class="panel-body">
    <div class="row">
      <strong>Status:</strong>
      <%= @lead.status %>

      <strong>Classificacao:</strong>
      <%= @lead.classificacao %>
    </div>
    <div class="row">
      <strong>Reconhecido:</strong>
      <%= @lead.reconhecido %>

      <strong>Motivo desqualificacao:</strong>
      <%= @lead.motivo_desqualificacao %>

      <strong>Convertido:</strong>
      <%= @lead.convertido %>

      <strong>Data da conversao:</strong>
      <%= @lead.data_da_conversao %>
    </div>
  </div>  
</div>
<div class="panel panel-info">
  <div class="panel-heading">
    <h6 class="panel-title">Informações do Lead</h6>
  </div>
  <div class="panel-body">
    <div class="row">
      <strong>Empresa:</strong>
      <%= @lead.empresa %>    
    </div>
    <div class="row">
      <strong>Saudacao:</strong>
      <%= @lead.saudacao %>

      <strong>Titulo:</strong>
      <%= @lead.titulo %>
    
      <strong>Nome:</strong>
      <%= @lead.nome %>
    
      <strong>Sobrenome:</strong>
      <%= @lead.sobrenome %>
    </div>
    <div class="row">
      <strong>Telefone:</strong>
      <%= @lead.telefone %>

      <strong>Celular:</strong>
      <%= @lead.celular %>

      <strong>Fax:</strong>
      <%= @lead.fax %>

      <strong>Recusar telefonemas:</strong>
      <%= @lead.recusar_telefonemas %>
    </div>
    <div class="row">
      <strong>Email:</strong>
      <%= @lead.email %>

      <strong>Recusar emails:</strong>
      <%= @lead.recusar_emails %>
    </div>
  </div>
</div>
