    </div>
    <div class="row">
      <div class="exibicao">
        <strong>Saudação:</strong>
        <%= @lead.saudacao %>
      </div>
      
      <div class="exibicao">
        <strong>Título:</strong>
        <%= @lead.titulo %>
      </div>
    
      <div class="exibicao">
        <strong>Nome:</strong>
        <%= @lead.nome %>
      </div>
    
      <div class="exibicao">
        <strong>Sobrenome:</strong>
        <%= @lead.sobrenome %>
      </div>
    </div>
    <div class="row">
      <div class="exibicao">
        <strong>Telefone:</strong>
        <%= @lead.telefone %>
      </div>

      <div class="exibicao">
        <strong>Celular:</strong>
