<div class="panel panel-info">
  <div class="panel-heading">
     <h6 class="panel-title">Estado</h6>
  </div>
  <div class="panel-body">
    <div class="row">
      <div class="exibicao">
        <strong>Status:</strong>
        <%= @lead.status %>
      </div>      
      <div class="exibicao">
        <strong>Classificacao:</strong>
        <%= @lead.classificacao %>
      </div>
    </div>
    <div class="row">
      <div class="exibicao">
        <strong>Reconhecido:</strong>
        <%= @lead.reconhecido %>
      </div>
      <div class="exibicao">
        <strong>Motivo desqualificacao:</strong>
        <%= @lead.motivo_desqualificacao %>
      </div>
      <div class="exibicao">
        <strong>Convertido:</strong>
        <%= @lead.convertido %>
      </div>
      <div class="exibicao">
        <strong>Data da conversao:</strong>
        <%= @lead.data_da_conversao %>
      </div>
    </div>
  </div>  
</div>
